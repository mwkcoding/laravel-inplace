{
    "name": "devsrv/laravel-inplace",
    "description": "inline edit with laravel blade components",
    "keywords": [
        "devsrv",
        "laravel-inplace",
        "Laravel"
    ],
    "homepage": "https://github.com/devsrv/laravel-inplace",
    "license": "MIT",
    "authors": [
        {
            "name": "Sourav Rakshit",
            "email": "hello@itsrav.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/contracts": "^7.0 || ^8.32",
        "illuminate/support": "^7.0 || ^8.32",
        "illuminate/view": "^7.0 || ^8.32"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "devsrv\\inplace\\": "src",
            "devsrv\\inplace\\Database\\Factories\\": "database/factories"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "devsrv\\inplace\\InplaceServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
